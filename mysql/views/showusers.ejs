<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Users</title>
</head>
<body>
<h1>All Users</h1>
<table border="1" cellpadding="10" cellspacing="0">
    <tr>
        <th>ID</th>
        <th>Username</th>
        <th>Email</th>
        <th>Edit</th>
        
    </tr>
    <% users.forEach(user => { %>
    <tr>
        <td><%= user.id %></td>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><form method="GET" action="/users/<%=user.id %>/edit"><button onclick="" >Edit</button></form></td>
      
    </tr>
    <% }) %>
</body>
</html>